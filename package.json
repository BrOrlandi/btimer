{
  "name": "btimer",
  "productName": "BTimer",
  "version": "1.0.0",
  "description": "A simple timer for web, mobile and desktop.",
  "main": "index.js",
  "repository": "git@github.com:BrOrlandi/BTimer.git",
  "author": "BrOrlandi",
  "license": "MIT",
  "private": false,
  "scripts": {
    "watch": "parcel watch ./src/index.html -d dist --public-url ./",
    "build": "rm -rf dist && parcel build ./src/index.html -d dist --public-url ./ ",
    "dev": "parcel serve ./src/index.html -o -d dist",
    "lint": "eslint index.js ./src",
    "deploy": "yarn build && ghpages -p dist"
  },
  "dependencies": {
    "moment": "^2.19.3",
    "vanilla-masker": "^1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "ghpages": "^0.0.10",
    "parcel-bundler": "^1.5.0",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5"
  }
}
